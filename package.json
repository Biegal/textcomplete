    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "events": "^1.1.0",
    "line-height": "^0.1.1",
    "lodash.assignin": "^4.0.3",
    "lodash.bindall": "^4.1.0",
    "lodash.isfunction": "^3.0.8",
    "lodash.isstring": "^4.0.1",
    "lodash.uniqueid": "^4.0.0",
    "textarea-caret": "^3.0.1"
